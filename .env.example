# Bitres Environment Configuration
# Copy this file to .env and fill in your values

# =============================================================================
# Sepolia Testnet
# =============================================================================
# RPC URL - Get from Infura, Alchemy, or use public endpoint
SEPOLIA_RPC_URL=https://rpc.sepolia.org

# Deployer private key (without 0x prefix is also accepted)
# WARNING: Never commit real private keys to git!
SEPOLIA_PRIVATE_KEY=

# =============================================================================
# Etherscan Verification
# =============================================================================
# Get API key from https://etherscan.io/apis
ETHERSCAN_API_KEY=

